doctype html
html
  head
    link(rel='stylesheet', type='text/css', href='./css/bootstrap-3.3.2-dist/css/bootstrap.css')
    link(href='./css/app.min.css', rel='stylesheet')
    script(src='https://maps.googleapis.com/maps/api/js?v=3.exp&libraries=places')
    script(src='https://ajax.googleapis.com/ajax/libs/jquery/1.11.2/jquery.min.js')
    title My Maps Dashboard
  body(class='site-wrapper')
    nav(class='navbar navbar-inverse')
      div(class='container-fluid')
        div(class='navbar-header')
          a(class='navbar-brand') Vagabond
        div(class='collapse navbar-collapse' id='bs-example-navbar-collapse-1')
          ul(class='nav navbar-nav')
            li 
              a(href='/createMaps') Create Maps
          ul(class='nav navbar-nav navbar-right')
            li 
              a(href='/del') Log Out
    h2(class='dashboard') My Maps          
    table(class='table table-hover mymaps')
      th Map Name 
      th URL
      each map in data
        tr
          td!= map.map.name
          td 
            a(href='maps/#{map.map.guid}', target='_blank') #{map.map.guid}
              
    a(href='/createMaps')
      button(type='button', class='btn btn-large btn-primary createmaps') Create New Map